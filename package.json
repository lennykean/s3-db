    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.13.14",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^3.4.4",
    "aws-sdk": "^2.475.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  },
  "maintainers": [
    {
      "name": "Matt Filion",
      "email": "matt.filion@gmail.com"
    }
