  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jeremyosborne/packagejsonexample/blob/master/LICENSE.txt"
    }
  ],
  "keywords": [
    "s3db",
    "aws",
    "aws s3",
    "s3",
    "database",
    "db",
    "s3 database",
    "s3 db"
