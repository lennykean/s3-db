    "test": "nyc ts-mocha ./spec/**/*",
    "format": "prettier --write \"dist/**/*.ts\" \"dist/**/*.js\" \"src/**/*.ts\"",
    "build": "npm run compile",
    "prepare": "npm run compile"
  },
  "dependencies": {
